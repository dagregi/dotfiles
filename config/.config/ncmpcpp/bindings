def_key "j"
	scroll_down
def_key "k"
	scroll_up

def_key "ctrl-u"
	page_up
def_key "ctrl-d"
	page_down

def_key "h"
    previous_column
def_key "l"
	next_column

def_key "n"
    next_found_item
def_key "N"
	previous_found_item

def_key "d"
	delete_playlist_items
def_key "d"
	delete_browser_items
def_key "d"
	delete_stored_playlist

def_key "J"
    move_sort_order_down
def_key "K"
	move_sort_order_up
def_key "h"
	jump_to_parent_directory
def_key "l"
	enter_directory
